{
  "trailingComma": "all",
  "printWidth": 120,
  "useTabs": false,
  "tabWidth": 2,
  "singleQuote": false,
  "bracketSpacing": true,
  "bracketSameLine": true,
  "arrowParens": "always",
  "embeddedLanguageFormatting": "auto",
  "semi": true,
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": [
    "^react(.*)$",
    "^react-dom$",
    "^(?:[./@]*next)(/.*|$)",
    "<THIRD_PARTY_MODULES>",
    "^(?:[./@]*app)(/.*|$)",
    "^(?:[./@]*components/base-ui)(/.*|$)",
    "^(?:[./@]*components/data-table/partials)(/.*|$)",
    "^(?:[./@]*components/data-table/data-controls)(/.*|$)",
    "^(?:[./@]*components/data-table/data-display)(/.*|$)",
    "^(?:[./@]*components/data-table)(/.*|$)",
    "^(?:[./@]*components/page-content/resources)(/.*|$)",
    "^(?:[./@]*components/page-content/auth)(/.*|$)",
    "^(?:[./@]*components/page-content)(/.*|$)",
    "^(?:[./@]*components/sheets)(/.*|$)",
    "^(?:[./@]*components/dialogs)(/.*|$)",
    "^(?:[./@]*components)(/.*|$)",
    "^(?:[./@]*hooks/resources)(/.*|$)",
    "^(?:[./@]*hooks/column-hooks)(/.*|$)",
    "^(?:[./@]*hooks)(/.*|$)",
    "^(?:[./@]*shared/lib)(/.*|$)",
    "^(?:[./@]*shared/constants)(/.*|$)",
    "^(?:[./@]*shared/enum)(/.*|$)",
    "^(?:[./@]*shared)(/.*|$)",
    "^(?:[./@]*types)(/.*|$)",
    "^(?:[./@]*)(/.*|$)",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "importOrderGroupNamespaceSpecifiers": true,
  "importOrderCaseInsensitive": true
}
