{
  "trailingComma": "all",
  "printWidth": 100,
  "tabWidth": 2,
  "singleQuote": false,
  "bracketSpacing": true,
  "bracketSameLine": true,
  "semi": true,
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": [
    "^react(.*)$",
    "^react-dom$",
    "<THIRD_PARTY_MODULES>",
    "^(?:[./@]*routes)(/.*|$)",
    "^(?:[./@]*hooks)(/.*|$)",
    "^(?:[./@]*components)(/.*|$)",
    "^(?:[./@]*utils)(/.*|$)",
    "^(?:[./@]*localization)(/.*|$)",
    "^(?:[./@]*constants)(/.*|$)",
    "^(?:[./@]*shared)(/.*|$)",
    "^(?:[./@]*types)(/.*|$)",
    "^(?:[./@]*styles)(/.*|$)",
    "^(?:[./@]*)(/.*|$)",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "importOrderGroupNamespaceSpecifiers": true,
  "importOrderCaseInsensitive": true
}
